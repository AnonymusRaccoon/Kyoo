{
  "server.urls": "http://0.0.0.0:5000",
  "https_port": 44300,
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",

  "databasePath": "/opt/kyoo/database.db",
